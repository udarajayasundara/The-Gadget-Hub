{
  "openapi": "3.0.1",
  "info": {
    "title": "TechWorld",
    "version": "1.0"
  },
  "paths": {
    "/api/Customer": {
      "post": {
        "tags": [
          "Customer"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerWriteDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerWriteDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerWriteDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Order": {
      "post": {
        "tags": [
          "Order"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OrderWriteDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/OrderWriteDTO"
              }
            },
            "application/*+json": {
